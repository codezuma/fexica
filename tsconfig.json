{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@lib/*": [
        "lib/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@icons/*": [
        "./icons/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@layout/*": [
        "./layout/*"
      ],
      "@ui/*": [
        "./components/ui/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
    }
  }
}