---
import CopyIcon from "@icons/copy-icon.astro";
import FacebookIcon from "@icons/facebook-icon.astro";
import TwitterIcon from "@icons/twitter-icon.astro";
import LinkedinIcon from "@icons/linkedin-icon.astro";
import Avatar from "@components/ui/avatar.astro";
const imageSrc = 'data:image/jpeg;base64,'
---

<section class="container-main flex md:flex-row flex-col gap-4 justify-between">
    <div class="flex gap-4">
        <Avatar src={imageSrc}/>
        <div>
            <h2 class="text-lg font-semibold text-gray-900">Rene Wells</h2>
            <h2 class=" text-gray-600">Content Writer</h2>
        </div>
    </div>
    <div class="flex gap-4 h-fit">
     <button class="button-outline">
        <CopyIcon/>
        &nbsp; copy link
     </button>
     <button class="button-outline"><FacebookIcon/></button>
     <button class="button-outline"><TwitterIcon/></button>
     <button class="button-outline"><LinkedinIcon/></button>
    </div>
</section>